<AnchorPane id="AnchorPane" prefHeight="653.0" prefWidth="851.0" style="-fx-background-color: #598ba4; -fx-background-radius: 90;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.EduMasVideoUtiController">
